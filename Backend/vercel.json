{
    "version": 2,
    "name": "mydrive",
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ],
    "functions": {
        "Backend/**/*.js": {
            "includeFiles": [
                "storage/**",
                "utils/**/*.json"
            ]
        }
    }
}